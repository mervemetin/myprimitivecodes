import java.util.Random;

public class RandomClass {
	public static void main(String[] args) {

		Random r = new Random();
		{
			int num1 = r.nextInt(6);
			num1 += 1;
			int num2 = r.nextInt(6);
			num2 += 1;
			System.out.println("Your random dice numbers :");
			System.out.println(num1 + " - " + num2);
		}
	}

}
